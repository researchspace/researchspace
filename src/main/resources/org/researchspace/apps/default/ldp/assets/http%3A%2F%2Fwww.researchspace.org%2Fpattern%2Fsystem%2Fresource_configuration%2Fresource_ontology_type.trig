    <http://www.researchspace.org/resource/system/fields/treePatterns> """{
  \"type\": \"full\",
  \"rootsQuery\": \"PREFIX skos: <http://www.w3.org/2004/02/skos/core#>\\nPREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>\\nPREFIX rso: <http://www.researchspace.org/ontology/>\\nPREFIX crm: <http://www.cidoc-crm.org/cidoc-crm/>\\nSELECT DISTINCT ?item ?label ?hasChildren WHERE {\\n  ?item a ?type .\\n  FILTER(?type in (rdfs:Class, owl:Class)).   \\n  FILTER(NOT EXISTS { ?item rdfs:subClassOf ?parent. })\\n  ?item ((crm:P1_is_identified_by/rdfs:label)|rso:displayLabel|rdfs:label|skos:prefLabel) ?label.\\n  FILTER (lang(?label) = \\\"en\\\")\\n  OPTIONAL { ?child rdfs:subClassOf ?item. }\\n  BIND(BOUND(?child) AS ?hasChildren)\\n  OPTIONAL { BIND(?label AS ?order) }\\n}\\nORDER BY (?order) (?label)\",
  \"childrenQuery\": \"PREFIX skos: <http://www.w3.org/2004/02/skos/core#>\\nPREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>\\nPREFIX rso: <http://www.researchspace.org/ontology/>\\nPREFIX crm: <http://www.cidoc-crm.org/cidoc-crm/>\\nSELECT DISTINCT ?item ?label ?hasChildren WHERE {\\n  ?item a ?type .\\n  FILTER(?type in (rdfs:Class, owl:Class)).  \\n      \\n  ?item rdfs:subClassOf ?parent.\\n  ?item ((crm:P1_is_identified_by/rdfs:label)|rso:displayLabel|rdfs:label|skos:prefLabel) ?label.\\n  FILTER (lang(?label) = \\\"en\\\")\\n      \\n  OPTIONAL { ?child rdfs:subClassOf ?item. }\\n  BIND(BOUND(?child) AS ?hasChildren)\\n  OPTIONAL { BIND(?label AS ?order) }\\n}\\nORDER BY (?order) (?label)\",
  \"parentsQuery\": \"PREFIX skos: <http://www.w3.org/2004/02/skos/core#>\\nPREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>\\nPREFIX rso: <http://www.researchspace.org/ontology/>\\nPREFIX crm: <http://www.cidoc-crm.org/cidoc-crm/>\\nSELECT DISTINCT ?item ?parent ?parentLabel WHERE {\\n  ?item a ?type .\\n  FILTER(?type in (rdfs:Class, owl:Class)).  \\n      \\n  ?item rdfs:subClassOf ?parent.\\n  ?parent ((crm:P1_is_identified_by/rdfs:label)|rso:displayLabel|rdfs:label|skos:prefLabel) ?parentLabel.\\n  FILTER (lang(?parentLabel) = \\\"en\\\")\\n}\",
  \"searchQuery\": \"PREFIX bds: <http://www.bigdata.com/rdf/search#>\\nPREFIX skos: <http://www.w3.org/2004/02/skos/core#>\\nPREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>\\nPREFIX rso: <http://www.researchspace.org/ontology/>\\nPREFIX crm: <http://www.cidoc-crm.org/cidoc-crm/>\\nSELECT DISTINCT ?item ?label ?score ?hasChildren WHERE {\\n  ?item a ?type .\\n  FILTER(?type in (rdfs:Class, owl:Class)).  \\n      \\n  ?item ((crm:P1_is_identified_by/rdfs:label)|rso:displayLabel|rdfs:label|skos:prefLabel) ?label.\\n    FILTER (lang(?label) = \\\"en\\\")\\n      \\n  ?label bds:search ?__token__;\\n    bds:minRelevance \\\"0.3\\\";\\n    bds:relevance ?score;\\n    bds:matchAllTerms \\\"true\\\".\\n  OPTIONAL { ?child rdfs:subClassOf ?item. }\\n  BIND(BOUND(?child) AS ?hasChildren)\\n  OPTIONAL { BIND(?label AS ?order) }\\n}\\nORDER BY DESC (?score) (?order) (?label)\\nLIMIT 200\"
}"""^^<http://www.researchspace.org/resource/system/syntheticJson>;
