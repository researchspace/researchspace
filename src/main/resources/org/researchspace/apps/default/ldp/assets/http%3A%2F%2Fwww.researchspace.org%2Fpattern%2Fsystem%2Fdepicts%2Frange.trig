    <http://spinrdf.org/sp#text> """SELECT ?value ?label WHERE {
    ?value a ?ontologyClass .
    ?ontologyClass rdfs:subClassOf* crm:E1_CRM_Entity .
  	{ 
      ?value crm:P1_is_identified_by ?appellation .
      ?appellation a crm:E41_Appellation .
      ?appellation crm:P2_has_type <http://www.researchspace.org/resource/system/vocab/resource_type/primary_appellation> . 
       
      ?appellation crm:P190_has_symbolic_content ?label .
	} UNION {
      ?value crm:P102_has_title ?title . 
      ?title a crm:E35_Title . 
      ?title crm:P2_has_type <http://www.researchspace.org/resource/system/vocab/resource_type/primary_title> . 
      ?title crm:P190_has_symbolic_content ?label .
	}
  FILTER REGEX(LCASE(STR(?label)), \"?token\", \"i\")
} ORDER BY ASC(?label) 
  LIMIT 10 """ .
  
