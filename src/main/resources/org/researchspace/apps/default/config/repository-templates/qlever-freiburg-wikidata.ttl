@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix config: <tag:rdf4j.org,2023:config/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix mph: <http://www.researchspace.org/resource/system/repository#> .
@prefix ephedra: <http://www.researchspace.org/resource/system/ephedra#> .

[] a config:Repository;
  config:rep.id "qlever-freiburg-wikidata";
  rdfs:label "QLever SPARQL repository";
  config:rep.impl [
      config:rep.type "researchspace:QLeverRepository";
      config:sparql.queryEndpoint <https://qlever.cs.uni-freiburg.de/api/wikidata>;
      mph:quadMode true;
      mph:writable true;
      mph:silentMode false
  ] .
