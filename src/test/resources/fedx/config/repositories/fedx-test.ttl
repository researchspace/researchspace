@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix config: <tag:rdf4j.org,2023:config/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix fed: <http://rdf4j.org/config/federation#> .

[] a config:Repository ;
   config:rep.id "fedx-test" ;
   rdfs:label "FedX Test Federation" ;
   config:rep.impl [
      config:rep.type "fedx:FedXRepository" ;
      fed:member [
         fed:store "ResolvableRepository" ;
         fed:repositoryName "repo1"
      ] ;
      fed:member [
         fed:store "ResolvableRepository" ;
         fed:repositoryName "repo2"
      ]
   ] .
