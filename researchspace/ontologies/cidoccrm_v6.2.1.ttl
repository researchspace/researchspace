  rdfs:label ""@zh, "a été la durée maximum de"@fr, "foi a duração máxima de"@pt, "war längste Dauer von"@de,
    "was maximum duration of"@en, "είναι μέγιστη διάρκεια του/της"@el, "был максимальной длительностью для"@ru;
  rdfs:range crm:E52_Time-Span .

crm:P87i_identifies a owl:ObjectProperty;
  rdfs:domain crm:E44_Place_Appellation;
  rdfs:label "bezeichnet"@de, "identifica"@pt, "identifie"@fr, "identifies"@en, "είναι αναγνωριστικό"@el,
