{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "additionalProperties": {
    },
    "properties": {
        "draggable": {
            "default": false,
            "description": "Specify if link should be draggable, e.g. into sets.",
            "type": "boolean"
        },
        "target": {
            "default": "'_self'",
            "description": "Equivalent to the `target` attribute of the `<a>` DOM element.\nCan be set to `_blank` to open the link in a new tab/window.",
            "enum": [
                "_blank",
                "_self"
            ],
            "type": "string"
        },
        "uri": {
            "description": "resource IRI to navigate",
            "type": "string"
        }
    },
    "propertyOrder": [
        "uri",
        "draggable",
        "target"
    ],
    "required": [
        "uri"
    ],
    "type": "object"
}

