@prefix config: <tag:rdf4j.org,2023:config/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ephedra: <http://www.researchspace.org/resource/system/ephedra#> .
@prefix : <http://www.researchspace.org/resource/system/service/vam/> .

[] a config:Repository ;
  config:rep.id "vam" ;
  rdfs:label "A wrapper for V&M using RESTSail repository" ;
  config:rep.impl [
    config:rep.type "openrdf:SailRepository" ;
    config:sail.impl [
      config:sail.type "researchspace:RESTSail" ;
      ephedra:serviceURL "https://api.vam.ac.uk/v2/objects/search" ;
      ephedra:httpMethod "GET" ;
      ephedra:implementsService :vam
    ] 
  ] .