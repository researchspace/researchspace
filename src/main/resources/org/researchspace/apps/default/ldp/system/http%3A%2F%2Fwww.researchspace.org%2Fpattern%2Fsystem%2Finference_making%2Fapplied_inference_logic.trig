  _:genid-236849f3638648a3bc7d91d61c5f0f8b-piv1cr a <http://spinrdf.org/sp#Query>;
    <http://spinrdf.org/sp#text> """DELETE {
  	$subject crminf:J3_applied $value . 
    $value crminf:J3i_was_applied_by $subject .
} WHERE {
  	$subject crminf:J3_applied $value . 
    $value crminf:J3i_was_applied_by $subject .
      
  ?value a ?ontologyClass .
  ?ontologyClass rdfs:subClassOf* crminf:I3_Inference_Logic .
      
  ?value crm:P1_is_identified_by ?appellation .
  ?appellation crm:P2_has_type <http://www.researchspace.org/resource/system/vocab/resource_type/primary_appellation> . 
  ?appellation crm:P190_has_symbolic_content ?label .
}""" .
  
  _:genid-236849f3638648a3bc7d91d61c5f0f8b-hpb8n a <http://spinrdf.org/sp#Query>;
    <http://spinrdf.org/sp#text> """SELECT DISTINCT ?value ?label WHERE {
  ?value a ?ontologyClass .
  ?ontologyClass rdfs:subClassOf* crminf:I3_Inference_Logic .
      
  ?value crm:P1_is_identified_by ?appellation .
  ?appellation crm:P2_has_type <http://www.researchspace.org/resource/system/vocab/resource_type/primary_appellation> . 
  ?appellation crm:P190_has_symbolic_content ?label .
      
  MINUS { ?value crm:P71i_is_listed_in|skos:inScheme ?systemAuthority . ?systemAuthority crm:P2_has_type Platform:System_Resource . }
  FILTER REGEX(LCASE(STR(?label)), \"?token\", \"i\")
} ORDER BY ASC(?label) 
  LIMIT 10 """ .
  
