  _:genid-f82a04d18d4b409a86d541597c949673-hivr54 a <http://spinrdf.org/sp#Query>;
    <http://spinrdf.org/sp#text> """DELETE {
  $subject crm:P62_depicts $value . 
  $value crm:P62i_is_depicted_by $subject .
} WHERE {
  $subject crm:P62_depicts $value . 
  $value crm:P62i_is_depicted_by $subject .
   	
    ?value a ?ontologyClass .
    ?ontologyClass rdfs:subClassOf* crm:E1_CRM_Entity .
    { 
      ?value crm:P1_is_identified_by ?appellation .
      ?appellation a crm:E41_Appellation .
      ?appellation crm:P2_has_type <http://www.researchspace.org/resource/system/vocab/resource_type/primary_appellation> . 
      ?appellation crm:P190_has_symbolic_content ?label .
    } UNION {
      ?value crm:P170i_time_is_defined_by ?timePrimitive .
      ?timePrimitive a crm:E61_Time_Primitive .
      ?timePrimitive crm:P2_has_type <http://www.researchspace.org/resource/system/vocab/resource_type/primary_time_primitive> .
      ?timePrimitive crm:P190_has_symbolic_content ?label .
    } UNION {
      ?value skos:prefLabel ?label .
    } UNION {
      ?value crm:P190_has_symbolic_content ?label .
    }
}""" .
  
