  rdfs:label "est ou a été possédée par"@fr, "has former or current owner"@en, "hat früheren oder derzeitigen Besitzer "@de,
    "é ou foi propriedade de"@pt, "έχει ή είχε ιδιοκτήτη"@el, "имеет бывшего или текущего владельца"@ru;
  rdfs:range crm:E39_Actor;
